<%= form_tag(consumables_path, method: :get) do %>
  <%= text_field_tag :term %>
  <%= submit_tag t(".search") %>
<% end %>
<div class="container">
  <div>
    <%= link_to t(".add consumable"), new_consumable_path, class: "btn btn-primary", remote: true %>
  </div>
  <div class="row" id="consumables_list">
    <%= render "consumables/consumables" %>
  </div>
</div>
<div id="consumable_modal"></div>
<%= link_to "English", locale: "en" %>
<%= link_to "Vietnamese", locale: "vi" %>
